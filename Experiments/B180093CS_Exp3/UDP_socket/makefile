make:
	gcc TCP_socket/client.c -o TCP_socket/client.out
	gcc TCP_socket/server.c -o TCP_socket/server.out
	gcc UDP_socket/client.c -o UDP_socket/client.out
	gcc UDP_socket/server.c -o UDP_socket/server.out

clean:
	rm TCP_socket/client.out
	rm TCP_socket/server.out
	rm UDP_socket/client.out
	rm UDP_socket/server.out
